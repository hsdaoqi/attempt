#include <stdio.h>
void bubble(int arr[],int sz)
{
  int i=0;
  for(i=0;i<sz-1;i++)
  {
    int j=0;
    for(j=0;j<sz-1-i;j++)
    {
      if(arr[j]>arr[j+i])
      {
      int tmp=arr[i];
      arr[i]=arr[i+1];
      arr[i+1]=tmp;
      }
    }
  }
}
int main(){
int arr[]={9,8,7,6,5,4,3,2,1};
int i,n;
int sz=sizeof(arr[])/sizeof(arr[0]);
bubble(arr,sz);
for(i=0;i<sz;i++)
{
printf("%d ",arr[i]);
}
   return 0;
}
